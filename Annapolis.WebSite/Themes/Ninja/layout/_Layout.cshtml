@using DevTrends.MvcDonutCaching

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div class="container-narrow">
    <div>@Html.Partial("_ScriptBeforeContent")</div>
    <div>
         @Html.Partial("_TopMenu")
    </div>
    <div class="jumbotron">
         @Html.Partial("_Jumbotron")
    </div>

    <div class="row-fluid marketing">
        @RenderBody()
    </div>
    <div>@Html.Partial("_ScriptAfterContent")</div>
    <div>@RenderSection("Scripts", required: false)</div>
    </div>
</body>
</html>
